{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && ( [ -d migrations ] && cp -r migrations dist/ || true ) && cp -r shared dist/ && cp deploy-fix.js . || true",
    "start": "NODE_ENV=production node deploy-fix.js && node dist/index.js",
    "postbuild": "echo '✅ البناء مكتمل'",
    "deploy": "npm run build",
    "check": "tsc",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "dup:check": "jscpd --reporters console --ignore \"**/node_modules/**,**/dist/**,**/.git/**\" .",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:push": "npx drizzle-kit push",
    "db:migrate-production": "node migrate-production.js",
    "db:check-production": "node check-production-db.js",
    "db:fix": "node apply-database-fix.js",
    "db:fix-production": "node fix-production-database.mjs",
    "db:add-profile-effect": "node run-profile-effect-migration.js",
    "db:fix-issues": "node fix-database-issues.js",
    "db:fix-data": "cd server && npx tsx scripts/fix-inconsistent-data.ts",
    "db:reset": "node scripts/reset-database.js",
    "auth:verify": "node scripts/verify-auth.js",
    "test:supabase": "tsx test-supabase-connection.ts",
    "clean-logs": "node tools/remove-console-logs.js",
    "build-production": "npm run clean-logs && npm run build",
    "smoke": "node scripts/smoke-api.js"
  },
